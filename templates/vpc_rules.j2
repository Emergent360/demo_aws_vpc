{
  "ec2_rules":
  {% for group in to_template_ec2_security_groups %}
  {
    "cidr_ip": "{{ group_defs.group.cidr_ip }}",
    "from_port": "{{ group_defs.group.from_port }}",
    "proto": "{{ group_defs.group.proto }}",
    "to_port": {{ group_defs.group.to_port }}
  }{% if not loop.last %},{% endif %}
  {% endfor %}
}